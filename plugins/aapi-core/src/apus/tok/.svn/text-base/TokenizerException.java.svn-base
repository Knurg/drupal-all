package apus.tok;

public class TokenizerException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 576730660231436348L;

	TokenizerException() {
		super();
	}

	TokenizerException(String message, Throwable cause) {
		super(message, cause);
	}

	TokenizerException(String message) {
		super(message);
	}

	TokenizerException(Throwable cause) {
		super(cause);
	}

	
}
